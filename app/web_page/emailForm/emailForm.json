    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.emailMessage ){",
        "\treturn {",
        "\t\t//map emailMessage variable to expected task contract input",
        "\t\temailMessageInput: {",
        "\t\t\tmessage: $data.emailMessage.message !== undefined ? $data.emailMessage.message : null,",
        "\t\t\temail: $data.emailMessage.email !== undefined ? $data.emailMessage.email : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "context" : {
